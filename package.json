{
  "name": "@octopus-synapse/profile-contracts",
  "version": "3.3.0",
  "description": "Shared domain contracts, schemas and types for Profile system",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octopus-synapse/profile-contracts.git"
  },
  "scripts": {
    "build": "bun run tsc",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "lint:fast": "oxlint src/",
    "precommit": "bun run typecheck && bun run lint:fast && bun test",
    "prepare": "husky"
  },
  "keywords": [
    "profile",
    "contracts",
    "schema",
    "zod"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@types/bun": "latest",
    "@types/node": "^20.10.6",
    "husky": "^9.1.7",
    "oxlint": "^1.38.0",
    "typescript": "^5.3.3"
  }
}
